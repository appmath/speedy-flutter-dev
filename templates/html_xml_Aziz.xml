<templateSet group="html/xml/Aziz">
  <template name="hco" value="&lt;!-- $EXP$ --&gt; $END$" description="HTML Comment" toReformat="true" toShortenFQNames="true">
    <variable name="EXP" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="XML" value="true" />
    </context>
  </template>
  <template name="importJS" value="&lt;head&gt;&#10;    &lt;script&#10;            src=&quot;$FILE_NAME$.js&quot;&#10;            type=&quot;text/javascript&quot;&gt;&#10;    &lt;/script&gt;&#10;&lt;/head&gt;&#10; $END$" description="Import a JS class/method" toReformat="true" toShortenFQNames="true">
    <variable name="FILE_NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="XML" value="true" />
    </context>
  </template>
</templateSet>
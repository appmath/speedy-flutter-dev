<templateSet group="JavaScriptAziz">
  <template name="dwsv" value="document.writeln(&quot;$STRING$: &quot; + $EXPR$);&#10;$END$ " description="document.writeln with STRING and a VARIABLE" toReformat="true" toShortenFQNames="true">
    <variable name="STRING" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="EXPR" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="dw" value="document.writeln($EXPR$);&#10;$END$" description="document.writeln" toReformat="true" toShortenFQNames="true">
    <variable name="EXPR" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="dwv" value="document.writeln(&quot;$EXPR$: &quot; + $EXPR$);&#10;$END$ " description="document.writeln with a VARIABLE" toReformat="true" toShortenFQNames="true">
    <variable name="EXPR" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="title" value="// --------- $STRING$ ---------------&#10; $END$" description="title comments to delimit important sections" toReformat="true" toShortenFQNames="true">
    <variable name="STRING" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="action" value="// ACTION: $STRING$&#10; $END$" description="action comment that specifies what I need to do" toReformat="true" toShortenFQNames="true">
    <variable name="STRING" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="docebyid" value="document.getElementById(&quot;$EXP$&quot;);&#10;$END$" description="document.getElementById" toReformat="true" toShortenFQNames="false">
    <variable name="EXP" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="vardoce" value="var $NAME$ = document.getElementById(&quot;$NAME$&quot;);&#10;$END$" description="var x = document.getElementById(" toReformat="true" toShortenFQNames="false">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="protoarg" value="$OBJECT$.prototype.$NAME$ = function($ARG$) {&#10;    this.$EXP$;&#10;};&#10;$END$&#10;" description="prototype with argument" toReformat="true" toShortenFQNames="false">
    <variable name="OBJECT" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ARG" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="EXP" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="proto" value="$OBJECT$.prototype.$NAME$ = function() {&#10;    this.$EXP$;&#10;};&#10;$END$&#10;" description="prototype" toReformat="true" toShortenFQNames="false">
    <variable name="OBJECT" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="EXP" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="obj" value=" var $OBJ$ = function($ARG$) {&#10;  this.$FIELD$ = $VALUE$;&#10;};  &#10;$END$&#10;&#10;" description="declare a var object" toReformat="true" toShortenFQNames="true">
    <variable name="OBJ" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ARG" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="FIELD" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="objarg" value=" var $OBJ$ = function($ARG$) {&#10;  this.$FIELD$ = $VALUE$;&#10;};  &#10;$END$&#10;&#10;" description="declare a var object with an argument" toReformat="true" toShortenFQNames="true">
    <variable name="OBJ" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ARG" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="FIELD" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="fun" value="function $NAME$() {&#10;  $END$&#10;}&#10;&#10;&#10;" description="function without argument" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_EXPRESSION" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="funA" value="function $NAME$($ARG$) {&#10;  $END$&#10;}&#10;&#10;&#10;" description="function with argument(s)" toReformat="true" toShortenFQNames="true">

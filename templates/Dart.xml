<templateSet group="Dart">
  <template name="fbv" value="void $NAME$($ARGS$) {&#10; $END$ &#10;}" description="void function with body" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;name&quot;" alwaysStopAt="true" />
    <variable name="ARGS" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="sout" value="print('$END$');" description="print to standard out" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="DART" value="true" />
    </context>
  </template>
</templateSet>